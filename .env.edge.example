# Edge Worker Configuration

# Proxy connection
PROXY_URL=https://cyrus-proxy.example.com
EDGE_TOKEN=your_edge_token_here

# Linear OAuth token (provided by proxy during setup)
LINEAR_OAUTH_TOKEN=your_oauth_token_from_proxy

# Local configuration
WORKSPACE_BASE_DIR=./workspaces
CLAUDE_PATH=/usr/local/bin/claude
PROMPT_TEMPLATE_PATH=./agent-prompt-template.md

# Debug flags (optional)
DEBUG_WEBHOOKS=false
DEBUG_EDGE=false